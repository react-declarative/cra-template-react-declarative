{
  "name": "cra-template-react-declarative",
  "version": "2.2.98",
  "description": "The most advanced CRM Boilerplate for React + TypeScript + Mobx. Contains snippets for automatic state management in a declarative style",
  "keywords": [
    "react",
    "create-react-app",
    "typescript",
    "mui",
    "material-ui",
    "mobx",
    "msw",
    "mock",
    "mocking",
    "state-management",
    "form-validation",
    "layout-engine",
    "grid-system",
    "declarative-programming",
    "json-endpoint",
    "crm",
    "ux"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/react-declarative/cra-template-react-declarative/issues"
  },
  "homepage": "https://github.com/react-declarative/cra-template-react-declarative#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-declarative/cra-template-react-declarative.git"
  },
  "scripts": {
    "start": "webpack-cli serve",
    "test": "rimraf test && npx create-react-app test --template=file:./",
    "msw-init": "npx msw init template/public/ --save"
  },
  "devDependencies": {
    "esbuild-loader": "2.19.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "html-webpack-plugin": "5.5.0",
    "rimraf": "3.0.2",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.0"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.0",
    "@mui/system": "5.11.0",
    "@types/node": "16.11.51",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-window": "1.8.5",
    "@types/uuid": "8.3.4",
    "clsx": "1.2.1",
    "dayjs": "1.11.5",
    "history": "5.3.0",
    "mobx": "6.6.2",
    "mobx-react": "7.5.3",
    "msw": "0.47.3",
    "notistack": "2.0.5",
    "path-to-regexp": "6.2.0",
    "react": "18.2.0",
    "react-declarative": "2.2.98",
    "react-dom": "18.2.0",
    "react-window": "1.8.7",
    "resize-observer-polyfill": "1.5.1",
    "tss-react": "4.3.4",
    "uuid": "8.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off",
      "new-parens": "off"
    }
  },
  "files": [
    "template",
    "template.json"
  ],
  "msw": {
    "workerDirectory": "template/public"
  }
}
